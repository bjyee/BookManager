<%= form_for @book do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :year_published %><br />
    <%= f.text_field :year_published %>
  </p>
  <p>
    <%= f.label :publisher_id %><br />
	<%= f.collection_select :publisher_id, Publisher.all, :id, :name %>
  </p>
  <p><%= render :partial => 'authors_checkboxes' %></p>
  <p><%= f.submit %></p>
<% end %>
